{
  "name": "webus",
  "version": "1.0.0",
  "description": "Bus ticket management toy application to showcase RuleKeeper's solution",
  "scripts": {
    "mongo:start": "docker run --name realworld-mongo -p 27017:27017 mongo & sleep 5",
    "start": "node bin/www",
    "start2": "node bin/www2",
    "debug": "DEBUG='RULEKEEPER'* DEBUG_COLORS=false node bin/www",
    "mongo:stop": "docker stop realworld-mongo && docker rm realworld-mongo"
  },
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.6",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "uuid": "^8.3.2"
  }
}